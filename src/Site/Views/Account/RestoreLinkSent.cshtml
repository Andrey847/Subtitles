@{
	ViewData["Title"] = "Login";
}

@using SubtitlesLearn.Site.Models;
@model RegisterViewModel

@section Css
	{
	<link rel="stylesheet" href="~/css/Account.css" asp-append-version="true">
}

@section Scripts {
	<script src='https://www.google.com/recaptcha/api.js?hl=en'></script>
	@{ await Html.RenderPartialAsync("_ValidationScriptsPartial"); }
	<script src="~/js/Account.js" asp-append-version="true"></script>
}


<img id="imgLogo" />
<h1 class="st-center">Restore password link sent to your email.</h1>
<a asp-action="Login" asp-controller="Acount">To login page</a>
